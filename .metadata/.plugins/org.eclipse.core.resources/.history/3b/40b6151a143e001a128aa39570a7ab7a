import java.util.Scanner;

public class While {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int num = sc.nextInt();

		while (num != 0) {
			System.out.println("Digite número inteiros positivos");

			
           num++;
		}
		System.out.println("Fim do programa");
		sc.close();
		
	}

}
